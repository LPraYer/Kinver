@model MVCKinver.Models.Genre

@{
    ViewBag.Title = "浏览产品";
}

<div class="container container-margin-top">
    <div class="row">
        <div class="span2">
            @{Html.RenderAction("GenreMenu","Store");}
        </div>
        <div class="span10">
            <div class="genre">
                <h3>
                    <em>@Model.Name</em>
                </h3>
                <ul id="product-list">
                    @foreach (var product in Model.Products)
                    { 
                        <li>
                            <a href="@Url.Action("Details", new { id = product.ProductId})">
                                <img class="img-polaroid" alt="@product.Title" src="@product.ProductImageUrl" />
                                <span>@product.Title</span>
                            </a>
                        </li>
                    }
                </ul>
            </div>
            <div id="test">
                33333
            </div>
        </div>
    </div>
</div>



