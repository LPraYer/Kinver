@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "HttpError";
}

<h2>HttpError</h2>

@if (Model != null)
{ 
    <p>
        @Model.Exception.GetType().Name
    </p>
    <p>
        thrown in @Model.ControllerName @Model.ActionName
    </p>
    <p>
        Error Details:
    </p>
    <p>
        @Model.Exception.Message
    </p>
}
